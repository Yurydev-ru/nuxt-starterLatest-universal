{
  "name": "nuxt4-starter-template",
  "description": "A minimal Nuxt 4 starter template with TypeScript, ESLint, Markdown support, and more.",
  "type": "module",
  "private": true,
  "author": {
    "name": "YuryDev",
    "email": "yurydevru@gmail.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0",
